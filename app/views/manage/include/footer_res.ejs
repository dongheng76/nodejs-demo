<script type="application/javascript">
    $(window).ready(function (){
        <%if(notice_info != null){%>
            <%if(notice_info.type == 'success'){%>
                noticeInfo('success','<%=notice_info.info%>');
            <%}else{%>
                noticeInfo('fail','<%=notice_info.info%>');
            <%}%>
        <%}%>
    });
</script>